{
  "name": "http-client-hints",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@9.12.2",
  "description": "HTTP Client Hints",
  "author": "userquin <userquin@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/userquin/http-client-hints#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/userquin/http-client-hints.git"
  },
  "bugs": "https://github.com/userquin/http-client-hints/issues",
  "keywords": [
    "HTTP Client Hints",
    "User Agent Client Hints",
    "Device Client Hints",
    "Network Client Hints",
    "Browser detection"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./critical": {
      "import": {
        "types": "./dist/critical.d.ts",
        "default": "./dist/critical.js"
      },
      "require": {
        "types": "./dist/critical.d.cts",
        "default": "./dist/critical.cjs"
      }
    },
    "./detect": {
      "import": {
        "types": "./dist/detect.d.ts",
        "default": "./dist/detect.js"
      },
      "require": {
        "types": "./dist/detect.d.cts",
        "default": "./dist/detect.cjs"
      }
    },
    "./device": {
      "import": {
        "types": "./dist/device.d.ts",
        "default": "./dist/device.js"
      },
      "require": {
        "types": "./dist/device.d.cts",
        "default": "./dist/device.cjs"
      }
    },
    "./h3": {
      "import": {
        "types": "./dist/h3.d.ts",
        "default": "./dist/h3.js"
      },
      "require": {
        "types": "./dist/h3.d.cts",
        "default": "./dist/h3.cjs"
      }
    },
    "./network": {
      "import": {
        "types": "./dist/network.d.ts",
        "default": "./dist/network.js"
      },
      "require": {
        "types": "./dist/network.d.cts",
        "default": "./dist/network.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "critical": [
        "./dist/critical.d.ts"
      ],
      "detect": [
        "./dist/detect.d.ts"
      ],
      "device": [
        "./dist/device.d.ts"
      ],
      "h3": [
        "./dist/h3.d.ts"
      ],
      "network": [
        "./dist/network.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "vitest",
    "lint": "eslint .",
    "lint:fix": "nr lint --fix",
    "release": "pnpm build && bumpp && npm publish"
  },
  "peerDependencies": {
    "h3": "^1.13.0"
  },
  "peerDependenciesMeta": {
    "h3": {
      "optional": true
    }
  },
  "dependencies": {
    "detect-browser-es": "^0.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@antfu/ni": "^0.23.0",
    "bumpp": "^9.2.0",
    "eslint": "^9.13.0",
    "h3": "^1.13.0",
    "publint": "^0.2.10",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  }
}
